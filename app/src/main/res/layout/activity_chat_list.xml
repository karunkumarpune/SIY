<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/layout_chat_list_recorder"
    tools:context="com.app.siy.activity.ChatListActivity">

    <include
        android:id="@+id/include_toolbar_chat_list_recorder"
        layout="@layout/toolbar_layout" />


    <android.support.v7.widget.RecyclerView
        android:id="@+id/chat_recycler_view_recorder"
        android:scrollbars="vertical"
        android:layout_below="@id/include_toolbar_chat_list_recorder"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </android.support.v7.widget.RecyclerView>
</RelativeLayout>
